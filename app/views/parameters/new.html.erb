<%= form_with url: "/process_steps/#{@process_step_id}/parameters", method: :post, local: true do |f| %>
  <%= f.label :measurement, "Measurement: " %>
  <%= f.number_field :measurement, step: :any %>

  <% f.number_field :process_step_id, value: @process_step_id %>
  <%= f.submit 'Submit' %>
<% end %>
