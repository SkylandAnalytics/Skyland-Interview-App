<h3>Edit Measurement</h3>

  <%= form_with(model: @parameter, url: process_step_parameter_path, method: :patch, local: true) do |f| %>
    <%= f.text_field :measurement, :value => @parameter.measurement.round(1) %>
    <%= f.submit "Save" %>
  <% end %><br><br>

<h3>Delete Measurement</h3>
<%= button_to "Delete", process_step_parameter_path, method: :delete, params: {process_step_id: @parameter.process_step_id} %>
